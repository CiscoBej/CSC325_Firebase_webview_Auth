<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csc325_firebase_webview_auth.SignUp">
    <children>
        <TextField fx:id="nameField" layoutX="227.0" layoutY="79.0" promptText="Name" />
        <PasswordField fx:id="passwordField" layoutX="227.0" layoutY="135.0" promptText="Password" />
        <TextField fx:id="emailField" layoutX="226.0" layoutY="188.0" promptText="Email" />
        <TextField fx:id="phoneField" layoutX="226.0" layoutY="246.0" promptText="Phone" />
        <Button fx:id="signUpButton" layoutX="272.0" layoutY="312.0" onAction="#handleSignUp" text="Sign Up" />
    </children>
</AnchorPane>
